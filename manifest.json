{
    "name": "hash password",
    "version": "1",
    "description": "hash password using php",
    "browser_action": {
        "name": "Manipulate DOM",
        "icons": [
            "laravel.png"
        ],
        "default_icon": "laravel.png",
        "default_popup": "home.html"
    },
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "jquery.min.js",
                "bootstrap.min.js",
                "content.js"
            ],
            "css": [
                "bootstrap.min.css"
            ]
        }
    ],
    "background": {
        "scripts": [
            "jquery.min.js",
            "background.js"
        ],
        "persistent": false
    },
    "manifest_version": 2
}